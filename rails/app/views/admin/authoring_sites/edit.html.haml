%h1 Edit Authoring Site

= form_for(@admin_authoring_site) do |f|
  - if @admin_authoring_site.errors.any?
  %ul
    - @admin_authoring_site.errors.map(&:full_message).each do |msg|
      %li= msg
  = render :partial => "form", :locals => {:f => f, :admin_authoring_site => @admin_authoring_site}

= link_to 'Back', admin_authoring_sites_path

