- if @form_model.available_projects.size > 0
  %h2.filterheader
    Collections
  .filter-group__options.webkit_scrollbars.projects
    - puts  @form_model.available_projects
    - @form_model.available_projects.sort_by { |p| p[:name].downcase }.each do |project|
      = check_box_tag 'project_ids[]', project[:id], @form_model.project_ids.include?(project[:id].to_s), id: "project_#{project[:id]}", class: 'check-box-left-margin'
      = label_tag "project_#{project[:id]}", project[:name]
      - if project[:landing_page_slug]
        = link_to image_tag("external_link.png", border: 0), project[:landing_page_slug], title: "Visit #{project[:name]} project page", target: "_blank"
      %br
