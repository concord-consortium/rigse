!!! XML
= render :partial => "otml/character_entities"
%otrunk{ :id => @mw_modeler_page.uuid}
  =ot_imports
  %objects
    %OTSystem{:local_id => 'system'}
      =ot_bundles(:title => h(@mw_modeler_page.name))
      %root
        %OTCompoundDoc
          %bodyText
            #content
              %h3
                = h(Embeddable::MwModelerPage.display_name + ': ' + @mw_modeler_page.name)
              %object{ :refid => ot_refid_for(@mw_modeler_page) }
      %library
        =yield :layout
